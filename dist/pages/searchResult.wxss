.container{font-size:24rpx;display:flex;flex-direction:column;height:100%}.container .loading{display:flex;flex-direction:column;justify-content:center;align-items:center}.container .loading text{color:#ccc;margin-top:50rpx}.container .result{padding:0 10rpx}.container .result .result-item{padding:20rpx 0;width:100%;margin:0 auto 5rpx;background-color:#EB6794;color:#fff}.container .result .result-item .title{width:500rpx;display:block;margin:0 auto;font-size:50rpx;text-align:center}.container .result .result-item .txt{width:500rpx;display:block;margin:0 auto;font-size:30rpx;text-align:center}.container .result .result-item .sound{width:65%;display:flex;flex-direction:row;margin:0 auto;justify-content:space-between}.container .result .result-item .sound view{width:150rpx;height:40rpx;background:#AC1D10;color:#fff;text-align:center;line-height:40rpx;border-radius:14rpx;font-size:20rpx}.container .result .result-item .sound view.female{background:#F5BDD1}